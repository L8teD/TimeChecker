<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:v="clr-namespace:TimeChecker.WPF.Views"
                    xmlns:vm="clr-namespace:TimeChecker.WPF.ViewModels">

    <DataTemplate DataType="{x:Type vm:DataViewerVM}">
        <v:DataViewer/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:TimePickerVM}">
        <v:TimePicker/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:CalendarVM}">
        <v:Calendar/>
    </DataTemplate>

</ResourceDictionary>